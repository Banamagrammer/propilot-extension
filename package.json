{
	"name": "propilot",
	"displayName": "Propilot",
	"description": "Pair programming with the greatest minds in the cyberverse",
	"version": "0.0.1",
	"publisher": "pro-gramming",
	"author": {
		"name": "The Banamagrammer",
		"email": "todo@todo.com",
		"url": "https://www.youtube.com/channel/UCpUXNjcIFVqVxbg8RxeO_HQ"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "propilot.ineedahepro",
				"title": "Halllp Me!"
			},
			{
				"command": "propilot.takepity",
				"title": "Take pity on the wretched"
			}
		],
		"configuration": {
			"title": "Propilot",
			"properties": {
				"propilot.imsopro": {
					"description": "I'm a pro and want to pilot amateurs into the skies of excellence",
					"type": "boolean",
					"default": false
				},
				"propilot.ineedahandleman": {
					"description": "You don't have an identity unless you have a handle",
					"type": "string",
					"default": "Anonygrammer",
					"maxLength": 30
				}
			}
		},
		"views": {
			"liveshare": [
				{
					"id": "pleas",
					"name": "Pleas",
					"contextualTitle": "Pleas for halllp",
					"when": "config.propilot.imsopro"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/ramda": "^0.27.44",
		"@types/vscode": "^1.57.0",
		"@types/ws": "^7.4.6",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"ramda": "^0.27.1",
		"vsls": "^1.0.3015",
		"ws": "^7.5.3"
	},
	"extensionDependencies": [
		"ms-vsliveshare.vsliveshare"
	]
}
